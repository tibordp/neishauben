{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "plugins": ["jest"],
    "globals": {
        "__webpack_public_path__": "writable"
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended",
        "plugin:jest/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
